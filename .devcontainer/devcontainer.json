{
  "name": "playground",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": [
    "--env-file",
    ".devcontainer/.env"
  ],
  "settings": {
    "editor.fontFamily": "Cascadia Code, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true
  },
  "extensions": [
    "ms-azuretools.vscode-docker",
    "denoland.vscode-deno",
    "eamodio.gitlens"
  ],
  "remoteUser": "vscode"
}